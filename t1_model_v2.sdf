    <!-- ##################################### LIDAR ##################################### -->

    <link name='lidar_link'>
        <pose relative_to="lidar_joint"/>
<!--        <pose>0.12 0 0.28 0 0 0</pose>-->
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.000167</ixx>
            <iyy>0.000167</iyy>
            <izz>0.000167</izz>
